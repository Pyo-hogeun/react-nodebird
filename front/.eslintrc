{
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        } 
    },
    "env":{
        "browser": true,
        "node": true
    },
    "extends": [
        "eslint:recommanded",
        "plugin:react/recommanded"
    ],
    "plugins": [
        "import",
        "react-hooks"
    ]
}